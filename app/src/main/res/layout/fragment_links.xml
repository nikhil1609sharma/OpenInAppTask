<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0E6FFF"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".views.fragment.LinksFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guide"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.12" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.03" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.05" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.95" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/figtree"
            android:textStyle="bold"
            android:text="Dashboard"
            android:textColor="@color/white"
            android:textSize="28sp"
            app:layout_constraintBottom_toBottomOf="@id/guide"
            app:layout_constraintStart_toStartOf="@id/guideStart"
            app:layout_constraintTop_toTopOf="@id/guideText" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/icon_back"
            android:backgroundTint="#2B80FF"
            android:padding="15dp"
            app:layout_constraintBottom_toBottomOf="@id/guide"
            app:layout_constraintEnd_toEndOf="@id/guideEnd"
            app:layout_constraintTop_toTopOf="@id/guideText">


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:src="@drawable/wrench" />

        </LinearLayout>


        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@drawable/dash_back"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@id/guide">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:orientation="vertical">

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guideGreetStart"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_percent="0.05" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guideGreetEnd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_percent="0.1" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/graphStart"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_percent="0.12" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/graphEnd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_percent="0.35" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/cardTop"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_percent="0.37"/>

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/cardBottom"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_percent="0.58" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guideLinks"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_percent="0.8" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/graph1End"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_percent="0.5" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guideVStart"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.05" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/guideVEnd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent="0.95" />


                <TextView
                    android:id="@+id/txtGreeting"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:fontFamily="@font/figtree"
                    android:textSize="18sp"
                    android:textColor="@color/black"
                    android:layout_marginStart="10dp"
                    app:layout_constraintBottom_toBottomOf="@id/guideGreetStart"
                    app:layout_constraintStart_toStartOf="@id/guideVStart"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/txtName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:textColor="@color/black"
                    android:layout_marginStart="10dp"
                    android:fontFamily="@font/figtree"
                    android:textStyle="bold"
                    android:text="User"
                    android:textSize="25sp"
                    app:layout_constraintStart_toStartOf="@id/guideVStart"
                    app:layout_constraintTop_toBottomOf="@id/txtGreeting" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardGraph"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    app:cardBackgroundColor="@color/white"
                    app:cardCornerRadius="10dp"
                    app:layout_constraintBottom_toBottomOf="@id/graphEnd"
                    app:layout_constraintEnd_toEndOf="@id/guideVEnd"
                    app:layout_constraintStart_toStartOf="@id/guideVStart"
                    app:layout_constraintTop_toTopOf="@id/graphStart">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">



                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="10dp"
                                android:fontFamily="@font/figtree"
                                android:text="@string/overview"
                                android:textColor="#A7A8AC"
                                android:textSize="13sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_gravity="end"
                                android:layout_marginStart="10dp"
                                android:layout_marginEnd="10dp"
                                android:paddingStart="10dp"
                                android:paddingEnd="10dp"
                                android:background="@drawable/time_outline_card"
                                android:drawableEnd="@drawable/time"
                                android:drawablePadding="10dp"
                                android:fontFamily="@font/figtree"
                                android:gravity="end"
                                android:padding="3dp"
                                android:text="22 Aug - 23 Sep"
                                android:textColor="#A7A8AC"
                                android:textSize="13sp" />


                        </RelativeLayout>

                        <com.github.mikephil.charting.charts.LineChart
                            android:id="@+id/graph"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:minHeight="150dp"
                            android:layout_marginTop="5dp" />


                    </LinearLayout>


                </androidx.cardview.widget.CardView>

                <HorizontalScrollView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:scrollbars="none"
                    android:layout_marginEnd="5dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginBottom="5dp"
                    android:nestedScrollingEnabled="true"
                    app:layout_constraintEnd_toEndOf="@id/guideVEnd"
                    app:layout_constraintStart_toStartOf="@id/guideVStart"
                    app:layout_constraintBottom_toBottomOf="@id/cardBottom"
                    app:layout_constraintTop_toBottomOf="@id/cardTop">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/card1"
                            android:layout_width="0dp"
                            android:minHeight="155dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:layout_marginStart="5dp"
                            android:minWidth="150dp"
                            app:cardBackgroundColor="@color/white"
                            android:layout_marginBottom="2dp"
                            app:cardCornerRadius="10dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="10dp"
                                    android:src="@drawable/click" />

                                <TextView
                                    android:id="@+id/todayCLick"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_weight="1.5"
                                    android:layout_marginTop="10dp"
                                    android:fontFamily="@font/figtree"
                                    android:textStyle="bold"
                                    android:text="123"
                                    android:gravity="bottom"
                                    android:textColor="#000000"
                                    android:layout_marginStart="10dp"
                                    android:textSize="17sp"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_weight="1.5"
                                    android:fontFamily="@font/figtree"
                                    android:layout_marginStart="10dp"
                                    android:gravity="center_vertical"
                                    android:textColor="#999CA0"
                                    android:text="Today's Click"
                                    android:textSize="17sp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/card2"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:minWidth="150dp"
                            android:minHeight="155dp"
                            android:layout_marginStart="15dp"
                            app:cardBackgroundColor="@color/white"
                            android:layout_marginBottom="2dp"
                            app:cardCornerRadius="10dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:weightSum="3"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="10dp"
                                    android:src="@drawable/pin" />

                                <TextView
                                    android:id="@+id/topLocation"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_weight="1.5"
                                    android:layout_marginTop="10dp"
                                    android:fontFamily="@font/figtree"
                                    android:textStyle="bold"
                                    android:text="Ahemdabad"
                                    android:gravity="bottom"
                                    android:textColor="#000000"
                                    android:layout_marginStart="10dp"
                                    android:textSize="17sp"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_weight="1.5"
                                    android:fontFamily="@font/figtree"
                                    android:layout_marginStart="10dp"
                                    android:gravity="center_vertical"
                                    android:textColor="#999CA0"
                                    android:text="Top Location"
                                    android:textSize="17sp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/card3"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:minHeight="155dp"
                            android:minWidth="150dp"
                            android:layout_marginStart="15dp"
                            app:cardBackgroundColor="@color/white"
                            android:layout_marginBottom="2dp"
                            app:cardCornerRadius="10dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:weightSum="3"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="10dp"
                                    android:src="@drawable/globe" />

                                <TextView
                                    android:id="@+id/topSource"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_weight="1.5"
                                    android:layout_marginTop="10dp"
                                    android:fontFamily="@font/figtree"
                                    android:textStyle="bold"
                                    android:text="Instagram"
                                    android:gravity="bottom"
                                    android:textColor="#000000"
                                    android:layout_marginStart="10dp"
                                    android:textSize="17sp"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_weight="1.5"
                                    android:fontFamily="@font/figtree"
                                    android:layout_marginStart="10dp"
                                    android:gravity="center_vertical"
                                    android:textColor="#999CA0"
                                    android:text="Top Source"
                                    android:textSize="17sp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/card4"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:minHeight="155dp"
                            android:layout_marginEnd="5dp"
                            android:minWidth="150dp"
                            android:layout_marginStart="15dp"
                            app:cardBackgroundColor="@color/white"
                            android:layout_marginBottom="2dp"
                            app:cardCornerRadius="10dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:weightSum="3"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="10dp"
                                    android:src="@drawable/link" />

                                <TextView
                                    android:id="@+id/totalLinks"
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_weight="1.5"
                                    android:layout_marginTop="10dp"
                                    android:fontFamily="@font/figtree"
                                    android:textStyle="bold"
                                    android:text="178"
                                    android:gravity="bottom"
                                    android:textColor="#000000"
                                    android:layout_marginStart="10dp"
                                    android:textSize="17sp"
                                    app:layout_constraintTop_toTopOf="parent" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="0dp"
                                    android:layout_weight="1.5"
                                    android:fontFamily="@font/figtree"
                                    android:layout_marginStart="10dp"
                                    android:gravity="center_vertical"
                                    android:textColor="#999CA0"
                                    android:text="Total Links"
                                    android:textSize="17sp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>


                    </LinearLayout>
                </HorizontalScrollView>

                <LinearLayout
                    android:id="@+id/llAnalytics"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:gravity="center_horizontal"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:paddingStart="20dp"
                    android:paddingEnd="20dp"
                    android:background="@drawable/back"
                    app:layout_constraintStart_toStartOf="@id/guideVStart"
                    app:layout_constraintEnd_toEndOf="@id/guideVEnd"
                    app:layout_constraintTop_toBottomOf="@id/cardBottom">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/arrows"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="View Analytics"
                        android:textSize="18sp"
                        android:layout_gravity="center_vertical"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:fontFamily="@font/figtree"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="20dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    app:layout_constraintStart_toStartOf="@id/guideVStart"
                    app:layout_constraintEnd_toEndOf="@id/guideVEnd"
                    app:layout_constraintBottom_toBottomOf="@id/guideLinks"
                    app:layout_constraintTop_toBottomOf="@id/llAnalytics">

                    <TextView
                        android:id="@+id/txtTopLinks"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Top Links"
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:paddingStart="20dp"
                        android:paddingEnd="20dp"
                        android:background="@drawable/round_back"
                        android:layout_gravity="center_vertical"
                        android:textSize="16sp"
                        android:textColor="#FFFFFF"
                        android:fontFamily="@font/figtree"/>

                    <TextView
                        android:id="@+id/txtRecentLinks"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Recent Links"
                        android:layout_marginStart="15dp"
                        android:layout_gravity="center_vertical"
                        android:textSize="16sp"
                        android:paddingTop="5dp"
                        android:paddingBottom="5dp"
                        android:paddingStart="10dp"
                        android:paddingEnd="10dp"
                        android:textColor="#999CA0"
                        android:fontFamily="@font/figtree"/>

                    <View
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"/>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/back"
                        android:layout_gravity="center_vertical|end"
                        android:padding="10dp">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/search" />

                    </LinearLayout>

                </LinearLayout>

                <ProgressBar
                    android:id="@+id/progressBar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/list"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    app:layout_constraintTop_toBottomOf="@id/guideLinks"
                    app:layout_constraintBottom_toBottomOf="parent">


                </androidx.recyclerview.widget.RecyclerView>



                <LinearLayout
                    android:id="@+id/llViewAllLinks"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:orientation="horizontal"
                    android:visibility="gone"
                    android:weightSum="2"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:gravity="center_horizontal"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingStart="20dp"
                    android:paddingEnd="20dp"
                    android:background="@drawable/back"
                    app:layout_constraintStart_toStartOf="@id/guideVStart"
                    app:layout_constraintEnd_toEndOf="@id/guideVEnd"
                    app:layout_constraintTop_toBottomOf="@id/cardBottom">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="8dp"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/link"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="View all Links"
                        android:textSize="18sp"
                        android:layout_gravity="center_vertical"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:fontFamily="@font/figtree"/>
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/llTalkWithUs"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="10dp"
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:paddingTop="20dp"
                    android:paddingBottom="20dp"
                    android:paddingStart="20dp"
                    android:paddingEnd="20dp"
                    android:background="@drawable/green_outline_back"
                    app:layout_constraintStart_toStartOf="@id/guideVStart"
                    app:layout_constraintEnd_toEndOf="@id/guideVEnd"
                    app:layout_constraintTop_toBottomOf="@id/cardBottom">

                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_marginEnd="8dp"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/whatsapp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Talk with us"
                        android:textSize="18sp"
                        android:textColor="@color/black"
                        android:layout_gravity="center_vertical"
                        android:fontFamily="@font/figtree"/>
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/llFAQS"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="100dp"
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:paddingTop="20dp"
                    android:paddingBottom="20dp"
                    android:paddingStart="20dp"
                    android:paddingEnd="20dp"
                    android:background="@drawable/blue_outline_back"
                    app:layout_constraintStart_toStartOf="@id/guideVStart"
                    app:layout_constraintEnd_toEndOf="@id/guideVEnd"
                    app:layout_constraintTop_toBottomOf="@id/cardBottom">

                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_marginEnd="8dp"
                        android:layout_gravity="center_vertical"
                        android:src="@drawable/faqs"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Frequently Asked Questions"
                        android:layout_gravity="center_vertical"
                        android:textSize="18sp"
                        android:textColor="@color/black"
                        android:fontFamily="@font/figtree"/>
                </LinearLayout>




            </LinearLayout>



        </androidx.core.widget.NestedScrollView>


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>